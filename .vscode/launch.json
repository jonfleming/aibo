    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Call Action API",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "aibo_api.js",
            "args": ["change_posture", "{\"FinalPosture\":\"sit_and_raise_both_hands\"}"]
        }        
    ]
}