{
    // "preLaunchTask": "vuejs: build",
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "attach",
            "name": "Node: Nodemon",
            "processId": "${command:PickProcess}",
            "restart": true,
            "protocol": "inspector"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch server",
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "server.js",
            "env": {"NODE_ENV":"development"},
            "preLaunchTask":"vuejs: serve"
        }   
    ]
}